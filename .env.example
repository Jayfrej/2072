# === NOTION CONFIGURATION ===
# Your Notion Integration Token (starts with 'ntn_' or 'secret_')
NOTION_TOKEN=

# Database ID (use the correct one from debug results)
DATABASE_ID=

# === SYNC CONFIGURATION ===
# How often to sync (in minutes)
SYNC_INTERVAL_MINUTES=15

# How many days back to look for trades
LOOKBACK_DAYS=30

# === MT5 ACCOUNTS ===
# Number of MT5 accounts to sync
MT5_ACCOUNT_COUNT=1

# === ACCOUNT 1 ===
ACCOUNT_1_NAME=Mix
ACCOUNT_1_LOGIN=
ACCOUNT_1_PASSWORD=
ACCOUNT_1_SERVER=
ACCOUNT_1_PATH=

